(define (day14-2 input)
  (let* ([groups '()]
         [knot-hashes (build-list 128 (λ (n) (main2 (string-append input "-" (number->string n)))))]
         [grid (list->string (flatten (map hash->bins knot-hashes)))]
         [final-groups (foldl (λ (n gs) (handle-cell n gs grid)) groups (range (string-length grid)))])
    (length final-groups)))

(define (handle-cell n groups grid)
  (if
   (equal? (string-ref grid n) #\1)
   (handle-cell* n groups)
   groups))

(define (handle-cell* n groups)
  (let* ([xy (n-xy n)]
         [adjs (get-adjacent (car xy) (cdr xy))])
     (let-values ([(adjacents others) (partition (λ (group) (ormap (λ (adj) (set-member? group adj)) adjs)) groups)])
       (cons (apply set-union (cons (set xy) adjacents)) others))))

(define (n-xy n)
  (cons (modulo n 128) (floor (/ n 128))))

(define max-dims 128)

(define (get-adjacent x y)
  (filter (λ (p) 
                  (or (equal? (car p) x)
                      (equal? (cdr p) y))) (filter (λ (p) (out-of-bounds p))
          (apply append (map
   (λ (f) (map f (build-list 3 (λ (y*) (sub1(+ y y*))))))
   (map (λ (e) (λ (z) (cons e z))) (build-list 3 (λ (x*) (sub1 (+ x x*)))))
  )))))
 

(define (out-of-bounds pair)
  (and
   (<= 0 (car pair) (sub1 max-dims))
   (<= 0 (cdr pair) (sub1 max-dims))))


